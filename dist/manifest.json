{
  "name": "مصباح",
  "description": "مولد النصوص العربية العشوائية",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "/images/mesba7-32.png",
      "128": "/images/mesba7-128.png"
    }
  },
  "icons": {
    "32": "/images/mesba7-32.png",
    "128": "/images/mesba7-128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://unpkg.com/vue@3 'unsafe-inline' 'unsafe-eval';"
  }
}
